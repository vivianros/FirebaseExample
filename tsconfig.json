{
    "compilerOptions": {
        "outDir": "ts_output_readonly_do_NOT_change_manually/",
        "rootDir": ".",
        
        "newLine": "CRLF",
        "module": "esnext",
        "target": "es5",
        "lib": ["es2017.object", "es6", "dom"],
        "sourceMap": true,
        "allowJs": true,
        "checkJs": true,
        "jsx": "react",
        "moduleResolution": "node",
        "baseUrl": ".",
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true
      },
    "formatCodeOptions": {
        "indentSize": 2,
        "tabSize": 2,
        "newLineCharacter": "\r\n",
        "convertTabsToSpaces": true,
        "insertSpaceAfterCommaDelimiter": true,
        "insertSpaceAfterSemicolonInForStatements": true,
        "insertSpaceBeforeAndAfterBinaryOperators": true,
        "insertSpaceAfterKeywordsInControlFlowStatements": true,
        "insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
        "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
        "placeOpenBraceOnNewLineForFunctions": false,
        "placeOpenBraceOnNewLineForControlBlocks": false
    },
    "files": [
        "firebase-messaging-sw.js",
        "src/simpleWebRTC.ts",
        "src/firebaseDbExample.ts",
        "src/firebaseWebRTC.ts",
        "src/firebaseRulesGenerator.ts",
        "src/firebasePushNotifications.ts",
        "src/dragAndDropHtmlExample.ts",
        "src/playground.ts",
        "typings/globals.d.ts",
        "typings/firebase.d.ts"
    ],
    "exclude": [
        "node_modules",
        "functions"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}